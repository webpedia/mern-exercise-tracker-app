{
  "name": "exercise-tracker-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "install": "yarn --cwd client",
    "client": "cd client && yarn start",
    "server": "nodemon ./bin/www",
    "build": "cd client && yarn build",
    "start": "npm-run-all -p client server",
    "production": "npm-run-all -s build server"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.10.15",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5"
  }
}
